EXE_INC = \
    -I$(OBJECTS_DIR)

LIB_LIBS = \
    $(FOAM_LIBBIN)/libOSspecific$(WM_CODI_AD_LIB_POSTFIX).o

ifeq (libo,$(FOAM_LINK_DUMMY_PSTREAM))
    LIB_LIBS += $(FOAM_LIBBIN)/dummy/libPstream$(WM_CODI_AD_LIB_POSTFIX).o
else
    LIB_LIBS += -L$(FOAM_LIBBIN)/dummy -lPstream$(WM_CODI_AD_LIB_POSTFIX)
endif

/* libz */
EXE_INC += -DHAVE_LIBZ

LIB_LIBS += -lz


/* Project lib dependencies. Never self-link (WM_PROJECT == OpenFOAM) */
PROJECT_LIBS =
