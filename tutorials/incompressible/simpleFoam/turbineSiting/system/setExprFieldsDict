FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setExprFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Preload any required fields (optional)
readFields      ( U );

_value1
{
    variables   ( "rho=1.2" );
    expression  #{ 0.5*rho*cbrt(mag(U)) #};
}

expressions
(
    windPowerDensity
    {
        field   windPowerDensity;
        create  yes;
        $_value1;
    }
);


